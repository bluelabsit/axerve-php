    "keywords": [
        "axerve",
        "zerogrey",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/bluelabsit/axerve-php",
    "license": "proprietary",
    "authors": [
        {
            "name": "ZeroGrey",
            "homepage": "https://www.zerogrey.com/"
        },
        {
            "name": "Bluelabs",
            "homepage": "https://www.bluelabs.it/"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
